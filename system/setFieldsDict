FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue kappa 0.48
);

regions
(
    boxToCell
    {
        box (0.02 0.072 -0.02) (0.02 0.05 -0.02);
        fieldValues
        (
            volScalarFieldValue kappa 1
        );
    }
);


