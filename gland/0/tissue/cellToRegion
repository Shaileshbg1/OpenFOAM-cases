FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/tissue";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 2;

boundaryField
{
    adiabatic_wall
    {
        type            zeroGradient;
    }
    skin
    {
        type            zeroGradient;
    }
    tissue_to_gland
    {
        type            calculated;
        value           uniform 0;
    }
}


