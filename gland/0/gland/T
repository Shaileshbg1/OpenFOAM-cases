FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/gland";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 37;

boundaryField
{
    ".*"
    {
        type            calculated;
        value           uniform 37;
    }
    thoraic_wall_gland
    {
        type            fixedValue;
        value           uniform 37;
    }
    gland_to_tissue
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        Tnbr            T;
        kappaMethod     solidThermo;
        value           uniform 37;
    }
    gland_to_tumor
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        Tnbr            T;
        kappaMethod     solidThermo;
        value           uniform 37;
    }
    gland_to_muscle
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        Tnbr            T;
        kappaMethod     solidThermo;
        value           uniform 37;
    }
}


