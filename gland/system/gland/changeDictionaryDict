boundary
{
    thoraic_wall_gland
    {
        type            patch;
    }
    
}

T
{
    internalField   uniform 300;

    boundaryField
    {
       
        gland_to_tissue
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;
            Tnbr            T;
            kappaMethod     solidThermo;
            value           uniform 300;
        }
       
        gland_to_tumor
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;
            Tnbr            T;
            kappaMethod     solidThermo;
            value           uniform 300;
        }

         gland_to_muscle
        {
            type            compressible::turbulentTemperatureCoupledBaffleMixed;
            Tnbr            T;
            kappaMethod     solidThermo;
            value           uniform 300;
        }
    }
}

